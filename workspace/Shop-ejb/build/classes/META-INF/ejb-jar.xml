<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd" version="3.2">
  <display-name>Shop-ejb</display-name>
  
  <enterprise-beans>
  	<session>
  		<ejb-name>VATCalculatorBean</ejb-name>
  		<env-entry>
  			<env-entry-name>vatRate</env-entry-name>
  			<env-entry-type>java.lang.Double</env-entry-type>
  			<env-entry-value>0.19</env-entry-value>
  		</env-entry>
  	</session>
  	<session>
  		<ejb-name>OrderStatisticBean</ejb-name>
  		<env-entry>
  			<env-entry-name>mailRecipient</env-entry-name>
  			<env-entry-type>java.lang.String</env-entry-type>
  			<env-entry-value>test@test.test</env-entry-value>
  		</env-entry>
  	</session>
  </enterprise-beans>
  
  <ejb-client-jar>Shop-common.jar</ejb-client-jar>
  
</ejb-jar>